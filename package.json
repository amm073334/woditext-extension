{
  "name": "woditext",
  "displayName": "woditext",
  "description": "syntax highlighting for woditext",
  "version": "0.0.1",
  "repository": "https://github.com/amm073334/woditext-extension",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "woditext",
      "aliases": ["woditext", "woditext"],
      "extensions": [".wod"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./wod_icon.png",
        "dark": "./wod_icon.png"
      }
    }],
    "grammars": [{
      "language": "woditext",
      "scopeName": "source.woditext",
      "path": "./syntaxes/woditext.tmLanguage.json"
    }]
  }
}
